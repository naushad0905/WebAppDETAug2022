@page
@using WebAppDETAug2022.Models
@model WebAppDETAug2022.Pages.IPLEventModel
@{
}
<h1> welcome to IPL 2022!!</h1>
<h2>Ticket Details</h2>



<table border=" 1">
    <tr>
        <th>Category </th>
        <th>Price </th>
        <th>MaxLimit </th>
    </tr>
    @foreach (Tickets t in Model.Tickets)
    {
        <tr>
            <td> <b> </b>@t.Category</td>
            <td> <b> </b>@t.price</td>
            <td> <b> </b>@t.MaxLimit</td>
        </tr>
    }
</table>
